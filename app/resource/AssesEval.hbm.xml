<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
   <class name="app.resource.AssesEval" table="asseseval">
      <id name="eval_id" type="int" >
         <generator class="identity"/>
      </id>
      <property name="test" type="string"/>
      <property name="enteredBy" type="string"/>
      <property name="assessmentDate" type="date"/>
      <property name="score" type="java.lang.Integer"/>
      <property name="approved" type="string"/>
      <property name="rejected" type="string"/>
      <property name="resourceId" type="java.lang.Integer"/>
      <property name="preferred" type="string"/>


   </class>
</hibernate-mapping>
