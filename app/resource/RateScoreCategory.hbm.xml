<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
	
<hibernate-mapping>
   <class name="app.resource.RateScoreCategory" table="ratescorecategory">
      <id name="rateScoreCategoryId" type="int" column="ID_RateScoreCategory">
         <generator class="identity"/>
      </id>
      
      <property name="category" type="string"/>      
            
   </class>
</hibernate-mapping>
