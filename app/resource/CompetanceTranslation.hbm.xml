        <property name="cc_certifiedIn" type="string"/>
        <property name="cc_expiration" type="string"/>
        <property name="cc_evidence_filename" type="string"/>
        <property name="cc_evidence_filepath" type="string"/>
        <property name="cultureNote" type="string"/>
