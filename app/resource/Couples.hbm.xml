<hibernate-mapping>
   <class name="app.resource.Couples" table="couples">
      <id name="id" type="int" >
         <generator class="identity"/>
      </id>
      <property name="resourceid" type="java.lang.Integer"/>
      <property name="resourcetype" type="string"/>
      <property name="coupleid" type="java.lang.Integer"/>
      <property name="coupletype" type="string"/>
      <property name="yesno" type="string"/>
      <property name="resourcename" type="string"/>
      <property name="couplename" type="string"/>


   </class>
</hibernate-mapping>
