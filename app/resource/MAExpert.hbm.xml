<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
	
<hibernate-mapping>
   <class name="app.resource.MAExpert" table="expertma">
      <id name="id_ma" type="int" column="id_ma">
         <generator class="identity"/>
      </id>
      
      <property name="id_resource" type="java.lang.Integer"/> 
      <property name="educationYears1" type="java.lang.Integer"/>                     
       <property name="educationYears2" type="java.lang.Integer"/>                     
      <property name="educationYears3" type="java.lang.Integer"/>                     
      <property name="professionYears" type="java.lang.Integer"/>                     
              
      <property name="contentExpert1" type="string"/>
      <property name="contentExpert2" type="string"/>
      <property name="contentExpert3" type="string"/>
      <property name="contentExpert4" type="string"/>
      <property name="contentExpert5" type="string"/>
      <property name="contentExpert6" type="string"/>
      <property name="education1" type="string"/>
      <property name="education2" type="string"/>
      <property name="education3" type="string"/>
      <property name="educationLevel1" type="string"/>
      <property name="educationLevel2" type="string"/>
   <property name="educationLevel3" type="string"/>
   <property name="profession" type="string"/>
   
      <property name="totalScore" type="java.lang.Double"/>  
       <property name="contentExpertScore" type="java.lang.Double"/>
        <property name="educatedInScore" type="java.lang.Double"/>
         <property name="professionScore" type="java.lang.Double"/>
   
   </class>
</hibernate-mapping>
