<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
   <class name="app.resource.ResourceClient" table="resourceClient">
      <id name="id" type="int" >
         <generator class="identity"/>
      </id>
      <property name="client" type="string"/>
      <property name="level" type="string"/>
      <property name="primary" type="string"/>
      <property name="primaryCount" type="string"/>
      <property name="secondry" type="string"/>
      <property name="secondryCount" type="string"/>
      <property name="resourceId" type="java.lang.Integer"/>
     
   </class>
</hibernate-mapping>
