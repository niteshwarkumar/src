       <set name="LanguagePairs" lazy="true" inverse="true" order-by="sourceId, targetId">
      	<key column="ID_Resource"/>
      	<one-to-many class="app.resource.LanguagePair"/>
      </set>      
           
      <set name="Unavails" lazy="true" inverse="true" order-by="startDate">
      	<key column="ID_Resource"/>
      	<one-to-many class="app.resource.Unavail"/>
      </set> 
      
      <set name="Industries" lazy="true" table="resource_industry" order-by="Industry_ID">
        <key column="ID_Resource"/>
        <many-to-many class="app.client.Industry" column="Industry_ID"/>
      </set>
      
       <set name="SpecificIndustries" lazy="true" table="resource_specificindustry" order-by="SpecificIndustry_ID">
        <key column="ID_Resource"/>
        <many-to-many class="app.client.SpecificIndustry" column="SpecificIndustry_ID"/>
      </set>
      
      <set name="ResourceTools" lazy="true" inverse="true" order-by="description">
      	<key column="ID_Resource"/>
      	<one-to-many class="app.resource.ResourceTool"/>
      </set> 
      
     <set name="ResourceContacts" lazy="false" inverse="true" order-by="lastName, firstName">
      	<key column="ID_Resource"/>
      	<one-to-many class="app.resource.ResourceContact"/>
      </set> 
      
      <set name="RateScoreDtps" lazy="true" inverse="true" order-by="application">
      	<key column="ID_Resource"/>
      	<one-to-many class="app.resource.RateScoreDtp"/>
      </set> 
      
      
      
