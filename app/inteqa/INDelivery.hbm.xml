<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="app.inteqa.INDelivery" table="in_delivery">
        <id name="id" type="int" column="id">
            <generator class="identity"/>
        </id>
        <property name="projectId" type="int"/>
        <property name="notes" type="string"/>
        <property name="caveats" type="string"/>

        <property name="verified" type="boolean"/>
        <property name="verifiedBy" type="string"/>
        <property name="verifiedDate" type="date"/>
        <property name="verifiedText" type="string"/>

        <property name="engverified" type="boolean"/>
        <property name="engverifiedBy" type="string"/>
        <property name="engverifiedDate" type="date"/>
        <property name="engverifiedText" type="string"/>


        <property name="dtpverified" type="boolean"/>
        <property name="dtpverifiedBy" type="string"/>
        <property name="dtpverifiedDate" type="date"/>
        <property name="dtpverifiedText" type="string"/>
        <property name="vendor" type="boolean"/>


    </class>
</hibernate-mapping>