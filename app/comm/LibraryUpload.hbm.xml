<hibernate-mapping>
   <class name="app.comm.LibraryUpload" table="libraryupload">
      <id name="libId" type="int" column="libId">
         <generator class="identity"/>
      </id>
      <property name="maintab" type="string"/>
      <property name="heading" type="string"/>
      <property name="title" type="string"/>
      <property name="description" type="string"/>
      <property name="uploadDate" type="date"/>
      <property name="uploadBy" type="string"/>
      <property name="format" type="string"/>
      <property name="fileName" type="string"/>
      <property name="fileSaveName" type="string"/>
      <property name="libMainId" type="int"/>
      <property name="htmlLink" type="string"/>
      <property name="clientId" type="int"/>

   </class>
</hibernate-mapping>
