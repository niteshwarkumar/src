<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
   <class name="app.project.QualityReport" table="qualityreport">
      <id name="qualityno" type="int" column="qualityno">
         <generator class="identity"/>
      </id>

      <property name="quality" type="string"/>
      <property name="qualityscore" type="string"/>
      <property name="qualitydesc" type="string"/>
      <property name="qualityyear" type="string"/>
     </class>

</hibernate-mapping>

