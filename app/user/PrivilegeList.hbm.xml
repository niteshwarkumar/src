<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
	
<hibernate-mapping>
   <class name="app.user.PrivilegeList" table="PrivilegeList">
      <id name="privilegeListId" type="int" column="ID_PrivilegeList">
         <generator class="identity"/>
      </id>
      
      <property name="privilege" type="string" not-null="true"/>         
            
   </class>
</hibernate-mapping>
