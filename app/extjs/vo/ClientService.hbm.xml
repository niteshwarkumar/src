<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
	
<hibernate-mapping>
   <class name="app.extjs.vo.ClientService" table="client_services">
      <id name="ID_Service" type="int" column="ID_Service">
         <generator class="identity"/>
      </id>
      <property name="ID_Client" type="int"/>         
      <property name="service" type="string"/>
      <property name="requirements" type="string"/>
    <property name="last_edited_id" type="string"/>

   </class>
</hibernate-mapping>
