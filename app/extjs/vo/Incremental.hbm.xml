<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
	
<hibernate-mapping>
   <class name="app.extjs.vo.Incremental" table="incrementals">
      <id name="incremental_id" type="int" column="incremental_id">
         <generator class="identity"/>
      </id>
      <property name="incDate" type="date"/>
      <property name="id_project" type="int"/>
      <property name="incDescription" type="string"/>
   </class>
</hibernate-mapping>
