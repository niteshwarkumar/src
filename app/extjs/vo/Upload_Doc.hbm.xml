<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
   <class name="app.extjs.vo.Upload_Doc" table="upload_doc">
      <id name="uploadDoc" type="int" >
         <generator class="identity"/>
      </id>
       <property name="ClientID" type="int"/>
       <property name="projectID" type="int"/>
       <property name="QuoteID" type="int"/>
       <property name="resourceId" type="int"/>
       <property name="pathname" type="string"/>
       <property name="path" type="string"/>
       <property name="uploadedBy" type="string"/>
       <property name="uploadDate" type="date"/>
       <property name="type" type="string"/>
       <property name="owner" type="string"/>
       <property name="title" type="string"/>
       <property name="description" type="string"/>
       <property name="docFormat" type="string"/>
       <property name="othId" type="int"/>

   </class>
</hibernate-mapping>
