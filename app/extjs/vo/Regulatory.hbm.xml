<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
	
<hibernate-mapping>
   <class name="app.extjs.vo.Regulatory" table="regulatory_client">
      <id name="regulatory_id" type="int" column="regulatory_id">
         <generator class="identity"/>
      </id>
      <property name="ID_Client" type="int"/>         
      <property name="regulatory_type" type="string"/>
      <property name="regulatory_requirement" type="string"/>
    <property name="edited_by" type="string"/>

   </class>
</hibernate-mapping>
