<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="app.qms.StrategicChange_Planner" table="strategicchange_planning">
        <id name="id" type="int" column="id">
            <generator class="identity"/>
        </id>

        <property name="scid" type="int"/>
        <property name="action" type="string"/>
        <property name="responsible" type="int"/>
        <property name="effectiveness" type="string"/>
        <property name="approver" type="int"/>
        <property name="approval" type="boolean"/>
        <property name="duedate" type="date"/>
        <property name="doi" type="date"/>
    </class>
</hibernate-mapping>
