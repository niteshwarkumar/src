<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="app.admin.PmFee" table="pmfee">
        <id name="id" type="int" column="id">
            <generator class="identity"/>
        </id>

        <property name="pmfee1" type="int"/>
        <property name="pmfee2" type="int"/>
        <property name="pmpercent" type="java.lang.Double"/>

    </class>
</hibernate-mapping>
