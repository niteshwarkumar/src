<hibernate-mapping>
    <class name="app.quote.Technical" table="technical">
        <id name="technicalId" type="int" column="ID_Technical">
            <generator class="identity"/>
        </id>
        <property name="clientQuote_ID" type="int"/>
        <property name="sourceOs" type="string"/>
        <property name="targetOs" type="string"/>
        <property name="sourceApplication" type="string"/>
        <property name="targetApplication" type="string"/>
        <property name="sourceVersion" type="string"/>
        <property name="targetVersion" type="string"/>
    </class>

</hibernate-mapping>